{
  "pulseaudio#output": {
    "format": "<span font_family=\"Symbols Nerd Font\" font_size=\"10pt\" rise=\"0.4pt\">{icon}</span><span font=\"Noto Sans 12px\">  {volume}<span font=\"Noto Sans 8px\" rise=\"2.5pt\">%</span></span>",
    "format-muted": "<span font_family=\"Symbols Nerd Font\" font_size=\"10pt\" rise=\"0.4pt\"></span><span font=\"Noto Sans 12px\"> MUTE</span>",
    "format-icons": [
      "",
      "",
      ""
